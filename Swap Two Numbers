# InvaderFDestiny
Personal Repository
#include <stdio.h>

int main()
{
    unsigned int num1,num2;
    printf("Enter the numbers?\n");
    scanf("%d%d",&num1,&num2);
    printf("Numbers before swapping-> num1=%d, num2+%d\n",num1,num2);
    num1^=num2^=num1^=num2;
    printf("Numbers after swapping-> num1=%d, num2=%d\n",num1,num2);
    return 0;
}
